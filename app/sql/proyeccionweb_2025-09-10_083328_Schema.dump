--
-- Name: validacionestemplate(integer, character varying, character varying); Type: FUNCTION; Schema: public; Owner: emelchor
--

CREATE FUNCTION public.validacionestemplate(p_institucion_id integer, pnivel character varying, pcodigo character varying) RETURNS text
    LANGUAGE plpgsql
    AS $$
DECLARE
    expresion TEXT := '';
    cuenta TEXT;
	tamanio integer;
	f record;
BEGIN
	expresion := '';
	if length(pcodigo) = 1 then
		tamanio := 3;
	end if;
	if length(pcodigo) = 3 then
		tamanio := 5;
	end if;
	if length(pcodigo) = 5 then
		tamanio := 7;
	end if;
	if length(pcodigo) = 7 then
		tamanio := 9;
	end if;
	
	raise notice 'Tamanio %  ',tamanio;
    for f in
	    SELECT * from public."templateBalance" where codigo like pcodigo || '%' and length(codigo) = tamanio  and institution_id=p_institucion_id
	  	loop
		   raise notice 'Conectando sucursal % % ',f.codigo,f.nivel;
		   expresion := expresion || ' + ' || f.codigo;
		end loop;
	return expresion;
END;
$$;


