CREATE FUNCTION public.generarvalidaciones(p_institucion_id integer) RETURNS void
    LANGUAGE plpgsql
    AS $$
DECLARE
    expresion TEXT := '';
    cuenta TEXT;
	f record;
BEGIN
	delete from validaciones_template where institucion_id = p_institucion_id;
    for f in
	    SELECT * from public."templateBalance" where institution_id=p_institucion_id
	  	loop
		   	raise notice 'Conectando sucursal % % ',f.codigo,f.nivel;
		  	select ValidacionesTemplate(p_institucion_id,f.nivel,f.codigo) into expresion;
			raise notice '      % ', expresion;
			if length(expresion) <> 0 then 
				INSERT INTO validaciones_template (
			        institucion_id, descripcion, cuenta_objetivo, expresion, operador, activo
			    ) VALUES (
			        p_institucion_id,
			        COALESCE(f.cuenta, 'Validaci√≥n: ' || expresion || ' = ' || f.codigo),
			        f.codigo,
			        expresion,
			        '=',
			        TRUE);
			end if;
		end loop;
END;
$$;


